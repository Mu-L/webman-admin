{
  "name": "workerman/webman",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "http://www.workerman.net",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "walkor",
      "email": "walkor@workerman.net",
      "homepage": "http://www.workerman.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "walkor@workerman.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "http://wenda.workerman.net/",
    "wiki": "http://workerman.net/doc/webman",
    "source": "https://github.com/walkor/webman"
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "workerman/webman-framework": "^1.3",
    "monolog/monolog": "^2.0",
    "topthink/think-orm": "^2.0",
    "tinywan/webman-event": "^0.0.2",
    "tinywan/validate": "^0.0.3",
    "tinywan/support": "dev-main",
    "illuminate/redis": "^8.83",
    "tinywan/exception-handler": "^0.0.5",
    "casbin/webman-permission": "^1.0",
    "tinywan/storage": "dev-main",
    "tinywan/jwt": "^1.1",
    "ext-gd": "*",
    "webman/console": "^1.0",
    "tinywan/captcha": "^0.0.1",
    "tinywan/curd": "dev-main",
    "tinywan/phinx": "dev-main"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "Tinywan\\Phinx\\" : "vendor/tinywan/phinx/src",
      "Tinywan\\Captcha\\" : "vendor/tinywan/captcha/src",
      "Tinywan\\Mailer\\" : "vendor/tinywan/mailer/src",
      "Casbin\\WebmanPermission\\" : "vendor/casbin/webman-permission/src",
      "Tinywan\\Curd\\" : "vendor/tinywan/curd/src",
      "Tinywan\\Nacos\\" : "vendor/tinywan/nacos/src",
      "" : "./"
    },
    "files": [
      "./support/helpers.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  }
}
