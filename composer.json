  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "workerman/webman-framework": "^1.2",
    "monolog/monolog": "^2.0",
    "topthink/think-orm": "^2.0",
    "webman/console": "^1.0",
    "tinywan/webman-event": "^0.0.2",
    "tinywan/casbin": "^0.0.5",
    "tinywan/validate": "^0.0.3",
    "tinywan/jwt": "^1.0",
    "tinywan/exception-handler": "^0.0.3"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "" : "./"
    },
    "files": [
      "./support/helpers.php"
